{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "eslint-disable no-const-assign": false,              
        "slint-disable no-return-assign": false,
        "slint-disable no-restricted-globals": false,
        "slint-disable no-undef ": false,
        "slint-disable react/destructuring-assignment ": false,
        "slint-disable prefer-template ": false,
        "slint-disable react/jsx-filename-extension ": false,
        "slint-disable react/react-in-jsx-scope": false

    }
}